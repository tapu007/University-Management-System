@model University.Repositories.Repository.UnAllocateAllClassRoomRepository

@{
    ViewData["Title"] = "UnallocateAllClassRoom";
}

<h1>Unallocate All Class Room</h1>

<hr />
<div class="row">
    <div class="col-md-4">   
        @if (ViewBag.Message != null)
        {
            <div class="alert-danger">@ViewBag.Message</div>
        }
        <form asp-action="UnallocateAllClassRoom">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="submit" value="UnAllocateAllClassRoom" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
