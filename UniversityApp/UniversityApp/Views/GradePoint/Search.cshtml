@model IEnumerable<University.Models.EntityModels.GradePoint>

@{
    ViewData["Title"] = "Search";
}

<h1>Search</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GradeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GradeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.GradePointId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.GradePointId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.GradePointId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
